#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/rollup@4.52.3/node_modules/rollup/dist/bin/node_modules:/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/rollup@4.52.3/node_modules/rollup/dist/node_modules:/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/rollup@4.52.3/node_modules/rollup/node_modules:/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/rollup@4.52.3/node_modules:/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/rollup@4.52.3/node_modules/rollup/dist/bin/node_modules:/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/rollup@4.52.3/node_modules/rollup/dist/node_modules:/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/rollup@4.52.3/node_modules/rollup/node_modules:/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/rollup@4.52.3/node_modules:/Users/saraaddeche/Desktop/ryans-files/a4/a4-components-a25/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
